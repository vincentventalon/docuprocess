{
    "baseUrl": "https://api.parsedocu.com",
    "headers": {
        "Content-Type": "application/json",
        "x-api-key": "{{connection.apiKey}}"
    },
    "log": {
        "sanitize": [
            "request.headers.x-api-key",
            "response.headers.x-api-key"
        ]
    },
    "response": {
        "error": {
            "message": "{{if(isArray(body.detail), body.detail[].msg, body.detail)}}"
        }
    }
}
