{
	"name": "n8n-nodes-yourapp",
	"version": "1.0.0",
	"description": "n8n community node for YourApp API",
	"keywords": [
		"n8n-community-node-package",
		"api",
		"yourapp"
	],
	"license": "MIT",
	"homepage": "https://www.parsedocu.com",
	"author": {
		"name": "ParseDocu",
		"email": "support@parsedocu.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/YourOrg/n8n-nodes-yourapp.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials",
		"lintfix": "eslint nodes credentials --fix",
		"prepublishOnly": "pnpm build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/YourAppApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/YourApp/YourApp.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.2.0",
		"typescript": "~5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
